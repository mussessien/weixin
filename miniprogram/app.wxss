.header{width: 750rpx;height: 150rpx;}
.hdCon{display: flex;justify-content: space-between;align-items: center;height: 100%;}
.hdCon image{height:90rpx;}
.hdCon navigator,.hdCon.kefu{height: 50rpx;}
.kefu{position: relative;}
.btn{position:absolute;width: 100%;height: 100%;top: 0;left: 0;opacity: 0;animation:dh 4s linear infinite alternate;}
.container{padding:0 26rpx 0 20rpx;box-sizing: border-box;}
@keyframes dh{
  0%,50%,100%{opacity: 1;}
  25%,75%{opacity: 0;}
}
.loge{display: flex;justify-content: space-between;align-items: center;}

